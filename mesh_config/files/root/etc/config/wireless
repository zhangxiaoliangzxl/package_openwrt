config wifi-device 'radio0'
	option type 'mac80211'
	option channel '6'
	option hwmode '11g'
	option path 'platform/qca956x_wmac'
	option htmode 'HT40'
	option radiotype '2g'
	option disabled '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'VHT80'
	option channel '149'
	option radiotype '5g'
	option disabled '0'

config wifi-iface 'wmesh'
	option device 'radio1'
	option ifname 'wlan_mesh'
	option mode 'mesh'
	option network 'lan'
	option mesh_id 'airocovmesh_20190701'
	option mesh_ttl '1'
	option mcast_rate '24000'
	option mesh_fwding '1'
	option mesh_rssi_threshold '-80'
	option disassoc_low_ack '0'
	option encryption 'sae'
	option key 'airocov20190701xxx'
	
config wifi-iface 'meshap'
	option ssid 'wirelessap'
	option device 'radio0'
	option mode 'ap'
	option disabled '1'
	option network 'lan'
	option ifname 'wlan_meshap'
	option encryption 'psk2+ccmp'
	option key 'airocov20200427'

